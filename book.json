{
"title": "Scratchapixel NoteBook",
"description": "阅读CG教程Scratchapixel的一些笔记",
"author": "whpointz",
"language": "zh-hans",
"gitbook": "3.2.3",
"root": ".",
"links": {
"sidebar": {
"Home": "https://whpointz.coding.me/whpointz"
}
},
"plugins": [
"-lunr",
"-search",
"-highlight",
"-livereload",
"search-plus@^0.0.11",
"simple-page-toc@^0.1.1",
"github@^2.0.0",
"prism@^2.1.0",
"prism-themes@^0.0.2",
"anchors@^0.7.1",
"include-codeblock@^3.0.2",
"ace@^0.3.2",
"emphasize@^1.1.0",
"katex@^1.1.3",
"tbfed-pagefooter@^0.0.1",
"anchor-navigation-ex@0.1.8",
"todo@^0.1.3"
],
"pluginsConfig": 
{
"prism": {
"css": [
"prism-themes/themes/prism-base16-ateliersulphurpool.light.css"
]
},
"github": {
"url": "https://github.com/whpointz/Scratchapixel-Notes"
},
"include-codeblock": {
"template": "ace",
"unindent": true,
"edit": true
},
"tbfed-pagefooter": {
"copyright": "Copyright whpointz 2017",
"modify_label": "The revision time:",
"modify_format": "YYYY-MM-DD HH:mm:ss"
},
"simple-page-toc": {
"maxDepth": 3,
"skipFirstH1": true
},
"anchor-navigation-ex": {
"isRewritePageTitle": false,
"tocLevel1Icon": "fa fa-hand-o-right",
"tocLevel2Icon": "fa fa-hand-o-right",
"tocLevel3Icon": "fa fa-hand-o-right"
}
}
}